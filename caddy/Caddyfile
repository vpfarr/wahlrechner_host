{$ROOT_DOMAIN} {
    header Content-Security-Policy: frame-ancestors *
    header -X-Frame-Options
    header Strict-Transport-Security max-age=31536000;

    handle_path /static/* {
        root * /www/dev/static
        file_server
    }
    
    handle_path /media/* {
        root * /www/dev/media
        file_server
    }

    root * /www/dev
    reverse_proxy dev:8000
}